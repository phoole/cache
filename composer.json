{
  "name": "phoole/cache",
  "type": "library",
  "license": "Apache-2.0",
  "version": "1.0.1",
  "description": "Simple cache implementation of PSR-16 for PHP",
  "keywords": [
    "phoole",
    "library",
    "php",
    "cache",
    "psr-16",
    "swoole"
  ],
  "authors": [
    {
      "name": "Hong Zhang",
      "homepage": "https://github.com/phoole"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "phoole/base": "^1.0.17",
    "psr/simple-cache": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  },
  "autoload": {
    "psr-4": {
      "Phoole\\Cache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phoole\\Tests\\": "tests/"
    }
  },
  "provide": {
    "psr/simple-cache-implementation": "^1.0"
  },
  "minimum-stability": "dev",
  "scripts": {
    "test": "phpunit",
    "check": "phpcs --standard=psr12 src/",
    "format": "phpcbf --standard=psr12 src/"
  }
}